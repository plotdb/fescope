//- module
doctype html
html
  block vars
  include @/@loadingio/bootstrap.ext/index.pug
  head
    block prehead
    +css("/assets/lib/bootstrap/main/dist/css/bootstrap.min.css")
    +css("/assets/lib/@loadingio/bootstrap.ext/main/index.min.css")
    +css("https://fonts.googleapis.com/css?family=Roboto:300,400,700|Roboto+Mono")
    +css("/assets/lib/ldcover/main/index.min.css")
    +css("/css/index.css")
    block head
  body
    block body
    +script([
      {name: "@loadingio/debounce.js"},
      {name: "@loadingio/ldquery"},
      {name: "@plotdb/semver"},
      {name: "@plotdb/rescope", version: "dev"}
    ])
    block script
